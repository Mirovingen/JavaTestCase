<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <!-- add servlet -->
    <servlet>
        <servlet-name>LastHourList</servlet-name>
        <servlet-class>app.servlets.LastHourList</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LastHourList</servlet-name>
        <url-pattern>/First</url-pattern>
    </servlet-mapping>

    <!-- add servlet -->
    <servlet>
        <servlet-name>Top5List</servlet-name>
        <servlet-class>app.servlets.Top5List</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Top5List</servlet-name>
        <url-pattern>/Second</url-pattern>
    </servlet-mapping>
</web-app>